  "scripts": {
    "start": "env-cmd -f .env.dev craco start",
    "build:dev": "env-cmd -f .env.dev craco build",
    "build:demo": "env-cmd -f .env.demo craco build",
    "build:app": "env-cmd -f .env.app craco build",
